<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="LearnTabbedPage.TabbedPageWithDataTemplate">
	<TabbedPage.ItemTemplate>
		<DataTemplate>
			<ContentPage Title="{Binding DisplayTitle}" Icon="{Binding IconName}">
				<ContentPage.Content>
					<StackLayout VerticalOptions="Center" HorizontalOptions="Center">
						<Label Text="{Binding DisplayName}">
						</Label>
					</StackLayout>
				</ContentPage.Content>
			</ContentPage>
		</DataTemplate>
	</TabbedPage.ItemTemplate>
</TabbedPage>